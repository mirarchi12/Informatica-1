#include <stdio.h>

int main(void)
{
    float nota;
    printf("Ingrese una calificaciÃ³n:");
    scanf ("%f", &nota);
    
    if (nota >= 90)
      printf ("Calificacion: A");
      else { if (nota >= 80)
              printf ("Calificacion:B");
              else { if (nota >= 70)
                      printf ("Calificacion: C");
                       else { if (nota >= 60)
                                printf ("Calificacion D");
                                else { if (nota < 60)
                                  printf ("Calificacion: F");
                                }
                       }
              }
      }
    return 0;
}
